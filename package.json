{
  "name": "cdd-ts",
  "version": "1.0.6",
  "description": "Test-time Contract Development framework for TypeScript",
  "main": "src/index.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "qa": "npm run lint &&npm run test&& npm run mutate",
    "prepublishOnly": "npm run qa&&tsc && rollup -c",
    "mutate": "stryker run",
    "test": "jest",
    "lint": "eslint . --ext .ts"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "fast-glob": "^3.2.12",
    "jest-mock": "^29.4.3",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@stryker-mutator/jest-runner": "^6.4.1",
    "@stryker-mutator/core": "^6.4.1",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jasmine-core": "~4.5.0",
    "jest": "^28.1.3",
    "jest-mock-extended": "^3.0.3",
    "rollup": "^3.18.0",
    "rollup-plugin-dts": "^5.2.0",
    "stryker-cli": "^1.0.2",
    "ts-jest": "^28.0.8",
    "typescript": "~4.8.2"
  }
}